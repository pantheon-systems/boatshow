        "tinify/tinify": "^1.5.2"
    },
    "require-dev": {
        "acquia/blt-require-dev": "11.x-dev",
        "drupal/console": "^1.0",
        "drupal/devel": "^2.0.0",
        "geerlingguy/drupal-vm": "^4.8"

    },
    "replace": {
        "webflo/drupal-core-require-dev": "*"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "web/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
          "drupal/admin_toolbar": {
              "Route view.media_library.page does not exist": "https://www.drupal.org/files/issues/2021-06-10/3218206-admin_toolbar-route_view_media_library_page-2.patch"
          },
          "drupal/fbip": {
                "Argument 2 passed to fbip_validator() must be an instance of FormStateInterface": "https://www.drupal.org/files/issues/fbip-2635716-4.patch"
            },
            "drupal/clientside_validation_jquery": {
                "Support inline_form_errors module": "https://www.drupal.org/files/issues/2906132-4.patch"
            },
            "drupal/clientside_validation": {
                "Add the library JS files locally instead of relying on CDN so big pipe works.": "./patches/clientside_validation-add-library.patch"
            },
            "drupal/core": {
                "Create a theme suggestion for taxonomy terms by view mode": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch",
                "Contextual links of reusable content blocks are not displayed when rendering entities built via Layout Builder [https://www.drupal.org/project/drupal/issues/3020876]": "https://www.drupal.org/files/issues/2020-02-22/contextual_links_layout_builder_0.patch",
                "PHP message: Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "https://www.drupal.org/files/issues/2019-08-17/drupal-layout_builder_inline_block_empty_block_error-3049332-8.7.x-11.patch",
                "Exception thrown by responsive srcset images when the image is not yet in the file system": "https://www.drupal.org/files/issues/2827921-remove-missing-responsive-image-width-exception.patch"
            },
            "drupal/redirect": {
                "Redirect hashes created when not retaining the query string": "https://www.drupal.org/files/issues/2018-06-01/2976930-redirect-hashes-created-when-not-retaining-the-query-string-2.patch"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "grasmash/drupal-security-warning": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "acquia/blt": true,
            "drupal/console-extend-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "nuke": [
            "rm -rf vendor composer.lock web/core web/modules/contrib web/profiles/contrib web/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
