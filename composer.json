        "drupal/addtoany": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.3",
        "drupal/anchor_link": "^2.5",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_field": "^1.0",
        "drupal/bricks": "^1.7",
        "drupal/captcha": "^1.0",
        "drupal/cer": "^4.0@alpha",
        "drupal/checklistapi": "^2.0",
        "drupal/chosen": "^2.5",
        "drupal/ckeditor_accordion": "^1.3",
        "drupal/ckeditor_readmore": "^2.0",
        "drupal/clientside_validation": "^3.0@RC",
        "drupal/cog": "^1.0.0",
