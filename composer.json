        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/rebuild_cache_access": "^1.5",
        "drupal/recaptcha": "^2.5",
        "drupal/redirect": "^1.4",
        "drupal/restui": "^1.19",
        "drupal/robotstxt": "^1.1",
        "drupal/rpt": "^1.0",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2",
        "drupal/simplesamlphp_auth": "^3.1",
        "drupal/smart_trim": "^1.1",
        "drupal/structure_sync": "^2.0",
        "drupal/telephone_formatter": "1.x-dev#64f2cbbb440044fce077e5ada35890388321fa5b",
        "drupal/token": "^1.9",
        "drupal/twig_field": "^1.0",
        "drupal/twig_tweak": "^2.9",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/username_enumeration_prevention": "^1.0",
        "drupal/video_embed_wistia": "^1.0@rc",
        "drupal/view_unpublished": "^1.0",
        "drupal/viewfield": "^3.0",
        "drupal/views_accordion": "^1.1",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/views_current_path": "^2.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/webform": "^5.23",
        "drupal/weight": "^3.1",
        "drupal/xmlsitemap": "1.x-dev#23b04ea6b7a577f5cc9531dcd18488bab01e65a6",
        "drush/drush": "^9.7",
        "giggsey/libphonenumber-for-php": "^8.10",
        "marketo-api/marketo-rest-client": "dev-master",
        "npm-asset/object-fit-images": "3.2.4",
        "npm-asset/picturefill": "^3.0.3",
        "symfony/property-access": "^4.3"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^10.0.0-alpha1",
        "geerlingguy/drupal-vm": "^5.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/bricks": {
                "Layout entity wrapper is missing [https://www.drupal.org/project/bricks/issues/2952523]": "https://www.drupal.org/files/issues/2018-04-12/bricks-layout_entity_wrapper-2952523-3.patch"
            },
            "drupal/clientside_validation": {
                "Add the library JS files locally instead of relying on CDN so big pipe works.": "./patches/clientside_validation-add-library.patch"
            },
            "drupal/clientside_validation_jquery": {
                "Support inline_form_errors module": "https://www.drupal.org/files/issues/2906132-4.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush [https://www.drupal.org/project/config_ignore/issues/2857247]": "https://www.drupal.org/files/issues/2019-09-27/support-for-export-2857247-38.patch"
            },
            "drupal/content_access": {
                "Undefined index: anonymous in content_access_get_role_gid": "./patches/content_access-role-2782813-19.patch"
            },
            "drupal/core": {
                "Create a theme suggestion for taxonomy terms by view mode": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch",
                "Exception thrown by responsive srcset images when the image is not yet in the file system (such as with Stage File Proxy) [https://www.drupal.org/project/drupal/issues/2827921]": "https://www.drupal.org/files/issues/2827921-remove-missing-responsive-image-width-exception.patch"
            },
            "drupal/fbip": {
                "Argument 2 passed to fbip_validator() must be an instance of FormStateInterface": "https://www.drupal.org/files/issues/fbip-2635716-4.patch"
            },
            "drupal/feeds": {
                "Entity Processor shows actions but uses wrong id [https://www.drupal.org/project/feeds/issues/3078857]": "https://www.drupal.org/files/issues/2019-09-03/3078857-2.patch",
                "HACK: ALWAYS CLEAN during processing": "./patches/feeds-always-clean.patch"
            },
            "drupal/lightning_media": {
                "Entity browser reordering of files broken in lightning 8.x-2.16 [https://www.drupal.org/project/lightning_media/issues/2901225]": "https://www.drupal.org/files/issues/2901225_20_lightning-media-module.patch"
            },
            "drupal/linkit": {
                "Link shown after the autocomplete selection is the bare node/xxx link": "https://www.drupal.org/files/issues/2018-03-09/2877535-7.linkit.Link-shown-after-the-autocomplete-selection-is-the-bare-nodexxx-link-not-the-alias.patch"
            },
            "drupal/marketo_ma": {
                "HACK: check for rest array key before accessing array": "./patches/marketo-ma-rest-array-check.patch"
            },
            "drupal/migrate_tools": {
                "Migrate support for deleting items no longer in the incoming data": "https://www.drupal.org/files/issues/2019-05-31/2809433-153.patch"
            },
            "drupal/redirect": {
                "Redirect hashes created when not retaining the query string": "https://www.drupal.org/files/issues/2018-06-01/2976930-redirect-hashes-created-when-not-retaining-the-query-string-2.patch"
            },
            "drupal/xmlsitemap": {
                "Rebuild always include all link types": "https://www.drupal.org/files/issues/2018-05-25/2745695-10.xmlsitemap-rebuild-all-entities.patch",
                "Add Drush 9 Support": "https://www.drupal.org/files/issues/2018-04-30/xmlsitemap--2968172-drush-9-support--2.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ],
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-saml.patch"
        ]
    }
}
