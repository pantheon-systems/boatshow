{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "acquia/blt": "^10.1.0",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/lightning": "^3.1.0",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_lift": "^4.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/address": "^1.7",
        "drupal/addtoany": "^1.9",
        "drupal/admin_toolbar": "^1.0",
        "drupal/admin_toolbar_tools": "^1.0",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/anchor_link": "^1.6",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/bricks": "1.x-dev#1ac993bffd03e223dd9a12c975fe2d89203b1aeb",
        "drupal/captcha": "1.x-dev#1d21e6864ced543312d8d819a170c3553281b485",
        "drupal/cer": "^4.0@alpha",
        "drupal/chosen": "^2.5",
        "drupal/ckeditor_accordion": "^1.1",
        "drupal/clientside_validation": "1.x-dev#d65b355a086812d7d3c84945da2a5df01b7815cf",
        "drupal/cog": "^1.0.0",
        "drupal/colossal_menu": "^1.1",
        "drupal/config_split": "^1.4",
        "drupal/core": "^8.6",
        "drupal/cshs": "1.x-dev#e583815ea8f0e11fd32c0b794700547cd3afce8d",
        "drupal/devel": "^2.0",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/eck": "^1.0@alpha",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "1.x-dev#83e4af15b3b8fd66390ae1545ccdfe6d1986dda9",
        "drupal/encryption": "^1.0",
        "drupal/eu_cookie_compliance": "^1.1",
        "drupal/faqfield": "^1.1",
        "drupal/features": "^3.8.0",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.0@beta",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geolocation": "^1.11",
        "drupal/gtm": "^1.1",
        "drupal/honeypot": "^1.27",
        "drupal/iframe": "^1.9",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imagemagick": "^2.3",
        "drupal/lightning_media": "^3.12",
        "drupal/linkit": "5.x-dev#1084887a0c4d9f0458c8d60f59ba20c342005101",
        "drupal/media_entity_browser": "2.x-dev#c75921d3af90f1b6dc052cd2edf037116227acf8",
        "drupal/media_entity_download": "^2.0@beta",
        "drupal/memcache": "2.0-alpha7",
        "drupal/menu_breadcrumb": "^1.7",
        "drupal/menu_item_extras": "^2.2",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0@beta",
        "drupal/module_filter": "^3.1",
        "drupal/module_missing_message_fixer": "^1.1",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.4",
        "drupal/purge": "^3.0@beta",
        "drupal/qa_accounts": "^1.0.0-alpha1",
        "drupal/rabbit_hole": "^1.0",
        "drupal/redirect": "^1.4",
        "drupal/restui": "^1.15",
        "drupal/robotstxt": "^1.1",
        "drupal/rpt": "1.0-beta1",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2.0",
        "drupal/smart_trim": "^1.1",
        "drupal/structure_sync": "^1.12",
        "drupal/telephone_formatter": "1.x-dev#64f2cbbb440044fce077e5ada35890388321fa5b",
        "drupal/twig_field": "^1.0",
        "drupal/twig_tweak": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/video_embed_wistia": "^1.0@rc",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_accordion": "^1.1",
        "drupal/views_block_filter_block": "^1.0@beta",
        "drupal/views_current_path": "^1.0@beta",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/webform": "^5.0@rc",
        "drupal/xmlsitemap": "1.x-dev#23b04ea6b7a577f5cc9531dcd18488bab01e65a6",
        "drush/drush": "^9.7",
        "giggsey/libphonenumber-for-php": "^8.10",
        "symfony/property-access": "^4.3"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^10.0.0-alpha1",
        "geerlingguy/drupal-vm": "^5.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/fbip": {
                "Argument 2 passed to fbip_validator() must be an instance of FormStateInterface": "https://www.drupal.org/files/issues/fbip-2635716-4.patch"
            },
            "drupal/viewfield": {
                "Duplicate reference field types": "https://www.drupal.org/files/issues/2900913-5-viewfield-fix-duplicates.patch"
            },
            "drupal/clientside_validation_jquery": {
                "Support inline_form_errors module": "https://www.drupal.org/files/issues/2906132-4.patch"
            },
            "drupal/clientside_validation": {
                "Add the library JS files locally instead of relying on CDN so big pipe works.": "./patches/clientside_validation-add-library.patch"
            },
            "drupal/eu_cookie_compliance": {
                "Make sure modal closes when refusing cookies.": "./patches/eu_cookie_compliance.patch"
            },
            "drupal/core": {
                "Create a theme suggestion for taxonomy terms by view mode": "https://www.drupal.org/files/issues/core-theme-suggestion-for-taxonomy-view-modes-2767243-14.patch"
            },
            "drupal/coder": {
                "Fix bad code sniffing in theme file": "https://www.drupal.org/files/issues/2018-05-10/filename-issue-theme-extension-2935388-4.patch"
            },
            "drupal/lightning_media": {
                "Entity browser reordering of files broken in lightning 8.x-2.16 [https://www.drupal.org/project/lightning_media/issues/2901225]": "https://www.drupal.org/files/issues/2901225_20_lightning-media-module.patch"
            },
            "drupal/linkit": {
                "Link shown after the autocomplete selection is the bare node/xxx link": "https://www.drupal.org/files/issues/2018-03-09/2877535-7.linkit.Link-shown-after-the-autocomplete-selection-is-the-bare-nodexxx-link-not-the-alias.patch"
            },
            "drupal/migrate_tools": {
                "Migrate support for deleting items no longer in the incoming data": "https://www.drupal.org/files/issues/2019-05-31/2809433-153.patch"
            },
            "drupal/redirect": {
                "Redirect hashes created when not retaining the query string": "https://www.drupal.org/files/issues/2018-06-01/2976930-redirect-hashes-created-when-not-retaining-the-query-string-2.patch"
            },
            "drupal/xmlsitemap": {
                "Rebuild always include all link types": "https://www.drupal.org/files/issues/2018-05-25/2745695-10.xmlsitemap-rebuild-all-entities.patch",
                "Add Drush 9 Support": "https://www.drupal.org/files/issues/2018-04-30/xmlsitemap--2968172-drush-9-support--2.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
