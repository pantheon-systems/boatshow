        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_discovery_override": "^1.0",
        "drupal/lightning_media": "^4.6",
        "drupal/linkit": "^5.0@beta",
        "drupal/login_security": "1.x",
        "drupal/marketo_ma": "2.x",
        "drupal/media_entity_browser": "2.x",
        "drupal/media_entity_download": "^2.0",
