{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "acquia/blt": "^10.1.0",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/lightning": "^3.1.0",
        "acquia/memcache-settings": "*",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.14",
        "drupal/admin_toolbar": "^1.0",
        "drupal/admin_toolbar_tools": "^1.0",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/advagg": "^4.0",
        "drupal/anchor_link": "^1.6",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^4.0",
        "drupal/block_field": "^1.0@alpha",
        "drupal/bricks": "^1.7-beta1",
        "drupal/captcha": "^1.0",
        "drupal/cer": "^4.0@alpha",
        "drupal/change_author_action": "^1.0",
        "drupal/chosen": "^2.5",
        "drupal/ckeditor_accordion": "^1.1",
        "drupal/clientside_validation": "1.x-dev#d65b355a086812d7d3c84945da2a5df01b7815cf",
        "drupal/cog": "^1.0.0",
        "drupal/colossal_menu": "^1.1",
        "drupal/config_filter": "^1.8",
        "drupal/config_ignore": "2.x-dev#2b1740411fd1eb8bb139711be5e10535c40ddbbf",
        "drupal/config_split": "^1.7",
        "drupal/content_access": "^1.0",
        "drupal/content_lock": "^2.2",
        "drupal/core": "^8.6",
        "drupal/cshs": "1.x-dev#e583815ea8f0e11fd32c0b794700547cd3afce8d",
        "drupal/devel": "^2.0",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/dropzonejs": "^2.4",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/eck": "^1.0@alpha",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "1.x-dev#83e4af15b3b8fd66390ae1545ccdfe6d1986dda9",
        "drupal/encryption": "^1.0",
        "drupal/faqfield": "^1.1",
        "drupal/features": "^3.8.0",
        "drupal/feeds": "3.x-dev#bb5a46ed51968af8e72bbf05e7c9ddf4fad7e4da",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.0@beta",
        "drupal/file_delete": "^1.0",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geolocation": "^3.0-rc1",
        "drupal/google_tag": "^1.2",
        "drupal/honeypot": "^1.27",
        "drupal/iframe": "^1.21",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/imageapi_optimize_binaries": "^1.0",
        "drupal/imageapi_optimize_resmushit": "^1.0",
        "drupal/imageapi_optimize_tinypng": "^1.1",
        "drupal/imagemagick": "^3.1",
        "drupal/lightning_media": "^3.12",
        "drupal/link_attributes": "^1.9",
        "drupal/linkit": "5.x-dev#1084887a0c4d9f0458c8d60f59ba20c342005101",
        "drupal/marketo_ma": "^2.0",
        "drupal/media_entity_browser": "2.x-dev#c75921d3af90f1b6dc052cd2edf037116227acf8",
        "drupal/media_entity_download": "^2.0@beta",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/menu_breadcrumb": "^1.7",
        "drupal/menu_item_extras": "^2.13",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.15",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0@beta",
        "drupal/module_filter": "^3.1",
        "drupal/module_missing_message_fixer": "^1.1",
        "drupal/paragraphs": "^1.8",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.4",
