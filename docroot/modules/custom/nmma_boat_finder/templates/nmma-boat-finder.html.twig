                <div class="js-filter-mobile-passengers-slider"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="mobile-filter-option" id="boatlength">
          <div class="filter-body">
            <div class="filter-description">
              <h3>Boat Length</h3>
            </div>
            <div class="filter-options">
              <div class="filter-range-option">
                <div class="js-filter-mobile-boat-length-slider"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="mobile-filter-option" id="propulsion">
          <div class="filter-body">
            <div class="filter-description">
              <h3>Engine Types</h3>
            </div>
            <div class="filter-options">
              <ul data-bind="foreach: propulsionList" class="clearfix">
                <li>
                  <label data-bind="click: triggerClick, css: ActiveClass, attr: { 'data-id': Id }">
                    <input type="checkbox" />
                    <i class="icon icon-db-check"></i>
                    <span data-bind="text: Name"></span>
                  </label>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="mobile-filter-option" id="trailerable">
          <div class="filter-body">
            <div class="filter-description">
              <h3>Transportation</h3>
            </div>
            <div class="filter-options">
              <ul data-bind="foreach: trailerabilityList" class="clearfix">
                <li>
                  <label data-bind="click: triggerClick, css: ActiveClass, attr: { 'data-id': Id }">
                    <input type="checkbox" />
                    <i class="icon icon-db-check"></i>
                    <span data-bind="text: Name"></span>
                  </label>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <div class="mobile-filters-footer">
        <div class="boat-matches-bottom"><span data-bind="text: NumberOfActiveBoats"></span> Boats match your preferences</div>
        <div><button data-modal-control="close" class="button">Apply Filters</button></div>
      </div>
    </div>
  </div>
</script>