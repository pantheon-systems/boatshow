          </div>
          <div class="slide">
            <div class="attribute-head">
              <h4>Length</h4>
            </div>
            <!-- ko foreach: ComparableBoats  -->
            <div class="mobile-compare-item flex-container">
              <div class="compare-item-boat">
                <div class="curve-corner--gray--white-lg">
                  <img data-bind="attr: { src: ImageUrl, alt: PluralName }">
                </div>
                <div class="compare-item-title">
                  <span data-bind="text: PluralName">All-Purpose Fishing Boat</span>
                </div>
              </div>
              <div class="mobile-compare-value">
                <span data-bind="html: LengthLabel"></span>
              </div>
            </div>
            <!-- /ko -->
            <!-- ko if: emptyComparisonSlot -->
            <div class="compare-header-empty-holder">
              You have room for <span data-bind='text: slotsLeft'></span> more <span data-bind='text: slotsLeftPlural'></span> in your comparison dock.
            </div>
            <!-- /ko -->
          </div>
          <div class="slide">
            <div class="attribute-head">
              <h4>Propulsion</h4>
            </div>
            <!-- ko foreach: ComparableBoats  -->
            <div class="mobile-compare-item flex-container">
              <div class="compare-item-boat">
                <div class="curve-corner--gray--white-lg">
                  <img data-bind="attr: { src: ImageUrl, alt: PluralName }">
                </div>
                <div class="compare-item-title">
                  <span data-bind="text: PluralName">All-Purpose Fishing Boat</span>
                </div>
              </div>
              <div class="mobile-compare-value">
                <ul data-bind="foreach: PropulsionNames">
                  <li data-bind="text: $data"></li>
                </ul>
              </div>
            </div>
            <!-- /ko -->
            <!-- ko if: emptyComparisonSlot -->
            <div class="compare-header-empty-holder">
              You have room for <span data-bind='text: slotsLeft'></span> more <span data-bind='text: slotsLeftPlural'></span> in your comparison dock.
            </div>
            <!-- /ko -->
          </div>
          <div class="slide">
            <div class="attribute-head">
              <h4>Trailerable</h4>
            </div>
            <!-- ko foreach: ComparableBoats  -->
            <div class="mobile-compare-item flex-container">
              <div class="compare-item-boat">
                <div class="curve-corner--gray--white-lg">
                  <img data-bind="attr: { src: ImageUrl, alt: PluralName }">
                </div>
                <div class="compare-item-title">
                  <span data-bind="text: PluralName">All-Purpose Fishing Boat</span>
                </div>
              </div>
              <div class="mobile-compare-value">
                <span data-bind="html: TrailableLabel"></span>
