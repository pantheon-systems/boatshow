          <!-- /ko -->
          <!-- ko if: emptyComparisonSlot -->
          <div class="compare-header-empty-holder">
          </div>
          <!-- /ko -->
          </div>
        </div>
      </div>
    </div>
    <div class="js-mobile-compare">
      <div class="mobile-compare-header">
        <div class="flex-container container">
          <h3>Compare Boats</h3>
          <a data-modal-control="close" href=""><i class="icon icon-db-close"></i></a>
