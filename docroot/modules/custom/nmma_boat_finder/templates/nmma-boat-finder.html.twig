<div class="page-frame en-us" id="boat-selector">
  <div class="boat-filter-block">
    <div class="boat-filter-mobile">
      <a data-bind="click: boatFinder.core.mobileFilters" href="#" class="js-mobile-filter-modal filter-modal-btn button--secondary">Search for Your Boat Now</a>
      {#<a data-bind="click: comparison.CompareTrigger" href="#" class="js-compare-modal-link compare-modal-btn btn-secondary-small">Compare</a>#}
      <div data-bind="'with': filters" class="boat-matches">
        <div class="active-boats"><span data-bind="text: NumberOfActiveBoats"></span> Boats match your preferences</div>
        <div>To compare features, check up to 4 boats below.</div>
      </div>
      <div style="display:none" class="compare-error">
        <div class="inner">
          <p data-bind="text: comparison.DesktopComparisonMessage"></p>
        </div>
      </div>
    </div>
    <!-- ko if: comparison.ComparableBoatsSelected -->
    <div class="boat-compare clearfix">
      <div class="container">
      <div data-bind="css: comparison.OpenComparisonDockClass" class="js-boat-compare-nav boat-compare-nav flex-container">
        <div class="label-container col-12-xs col-2-lg">
          <div>
            <span data-bind="html: comparison.ComparableBoatsLength">0</span> <span>of 4 Boat Types Selected</span>
          </div>
        </div>
        <div class="boat-compare-nav-body col-12-xs col-8-lg flex-container">
          <!-- ko foreach: comparison.ComparableBoatsViewModel  -->
          <div data-bind="css: ActiveClass " class="compare-nav-item col-3-md">
            <span class="compare-item">
              <span data-bind="with:Boat" class="inner">
                <div class="curve-corner--gray">
                  <img data-bind="attr: { src: ImageUrl, alt: PluralName }">
                </div>
                <a data-bind="click: CompareTrigger" href="#" class="item-remove" title="Remove"><i class="icon icon-db-close"></i></a>
                <a data-bind="text: PluralName, attr: { href: BoatDetailsUrl }" class="compare-item-title"></a>
              </span>
            </span>
          </div>
          <!-- /ko -->
        </div>
        <div class="button-container col-12-xs col-2-lg">
          <a data-bind="css: comparison.CanBeComparedClass, click: comparison.CompareTrigger" href="#" class="js-compare-modal-link button">Compare</a>
        </div>
      </div>
    </div>
  </div>
  <!-- /ko -->

    <div data-bind="'with': filters" id="boat-filter" class="boat-filter clearfix">
      <div class="filter-mobile-header">
        <div class="filter-mobile-header-inner">
          <div class="filter-items-count">
            <div class="items-count" data-bind="text: NumberOfActiveBoats">22</div>
            <div class="items-count-text"></div>
          </div>
          <a class="btn-secondary-small" href="#">apply filters</a>
          <a href="#" class="js-mobile-filter-modal filter-close">Close</a>
        </div>
      </div>

      <div class="boat-finder__tabs-wrapper">
        <ul>
          <li><h5><a href="#activities"><i class="icon icon-db-circle-activities-o"></i>Activities</a></h5></li>
          <li><h5><a href="#passengers"><i class="icon icon-db-circle-passengers-o"></i>Passengers</a></h5></li>
          <li><h5><a href="#boatlength"><i class="icon icon-db-circle-boat-o"></i>Boat Length</a></h5></li>
          <li><h5><a href="#propulsion"><i class="icon icon-db-circle-propeller-o"></i>Propulsion</a></h5></li>
          <li><h5><a href="#trailerable"><i class="icon icon-db-circle-trailer-o"></i>Trailerable</a></h5></li>
        </ul>
        <div id="activities">
          <div class="filter-body">
            <div class="filter-description">
              <strong>What activities are you interested in</strong>
            </div>
            <div class="filter-options">
              <ul data-bind="foreach: activityList" class="clearfix">
                <li>
                  <label data-bind="click: triggerClick, css: ActiveClass, attr: { 'data-gtm-tracking': GTM, 'data-id': Id, 'class': 'toggle-wrapper' }">
                    <input type="checkbox" />
                    <i class="icon icon-db-check"></i>
                    <span data-bind="text: Name"></span>
                  </label>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div id="passengers">
          <div class="filter-body">
            <div class="filter-description">
              <strong>How many people do you want in your boat?</strong>
            </div>
            <div class="filter-options">
              <div class="filter-range-option">
                <div class="js-filter-passengers-slider"></div>
              </div>
            </div>
          </div>
        </div>
        <div id="boatlength">
          <div class="filter-body">
            <div class="filter-description">
              <strong>How long do you want boat to be?</strong>
            </div>
            <div class="filter-options">
              <div class="filter-range-option">
                <div class="js-filter-boat-length-slider"></div>
              </div>
            </div>
          </div>
        </div>
        <div id="propulsion">
          <div class="filter-body">
            <div class="filter-description">
              <strong>What types of engines are interested in?</strong>
            </div>
            <div class="filter-options">
              <ul data-bind="foreach: propulsionList" class="clearfix">
                <li>
                  <label data-bind="click: triggerClick, css: ActiveClass, attr: { 'data-gtm-tracking': GTM, 'data-id': Id, 'class': 'toggle-wrapper' }">
                    <input type="checkbox" />
                    <i class="icon icon-db-check"></i>
                    <span data-bind="text: Name"></span>
                  </label>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div id="trailerable">
          <div class="filter-body">
            <div class="filter-description">
              <strong>How do you want to transport your boat?</strong>
            </div>
            <div class="filter-options">
              <ul data-bind="foreach: trailerabilityList" class="clearfix">
                <li>
                  <label data-bind="click: triggerClick, css: ActiveClass, attr: { 'data-gtm-tracking': GTM, 'data-id': Id, 'class': 'toggle-wrapper' }">
                    <input type="checkbox" />
                    <i class="icon icon-db-check"></i>
                    <span data-bind="text: Name"></span>
                  </label>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div data-bind="with: filters" class="boat-filter-keywords clearfix">
      <!-- ko 'if': ShowReset -->
      <strong class="filters-label">Filter</strong>
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', data: maxCapacity } -->
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', data: boatLength } -->
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', data: boatPrice } -->
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', foreach: activityList } -->
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', foreach: propulsionList } -->
      <!-- /ko -->

      <!-- ko template: { name: 'filter-keyword', foreach: trailerabilityList } -->
      <!-- /ko -->
    </div>

    <div data-bind="with: filters" class="boat-filter-keywords clearfix">
      <!-- ko 'if': ShowReset -->
      <a data-bind="click: triggerFilterReset" href="#" class="btn-secondary-small btn-reset-filters">reset all</a>
      <!-- /ko -->
    </div>
  </div>

  <div class="page-inner clearfix">
    <div class="main-content clearfix">
      <div class="center-content">
        <div class="user-messaging" data-bind="visible: widenSearchMessageVisible()" style="display: none;">
          <p>Sorry, your criteria did not return any results. Please widen your search....</p>
        </div>
        <div class="boat-item-boxes" id="boats-container">
          <!-- ko template: { name: 'boat-template', foreach: boatList, afterRender: BoatsAfterRender } -->
          <!-- /ko -->
          <div class="spacer boat-item-box col-12-xs col-6-sm col-4-md"></div>
          <div class="loading-wrapper">
            <div class="loading-big">
              Loading...
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/html" id="compare-modal-content">
