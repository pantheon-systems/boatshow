<script type='text/template' id='boat-template'>
  <div data-bind='visible: IsVisible' class='boat-item-box col-12-xs col-6-sm col-4-md'>
    <a data-bind='attr: { href: BoatDetailsUrl }'>
      <div class='curve-corner'>
        <img data-bind='attr: { src: ImageUrl, alt: PluralName }'>
      </div>
      <h5 class='icon icon-db-arrow-right'><span data-bind='text:PluralName'></span></h5>
    </a>
    <div class='boat-item-box-actions'>
      <a data-bind='css: ComparisonActiveClass, click: OpenComparison' href='#' class='boat-item-box-compare'>
        <i class='icon icon-db-circle-plus-o'></i><span data-bind='text: CompareLabel'>Compare</span>
      </a>
    </div>
  </div>
</script>

<script type='text/template' id='filter-keyword'>
  <!-- ko if: IsActiveFilter -->
  <span class='filter-keyword button'>
    <span data-bind='text:CompareLabel' class='filter-keyword-title'></span>
    <a data-bind="attr: { 'data-gtm-tracking': RemoveFilterGTM }, click: removeFilter" class='filter-keyword-remove' href='#'><i class='icon icon-db-close'></i></a>
  </span>
  <!-- /ko -->
</script>

