        </div>

        <div class="compare-row flex-container">
          <div class="compare-filter-title filter-propulsion col-3-md">
            <i class="icon icon-db-circle-propeller-o"></i> <h5 class="filter-header-title">Propulsion</h5>
          </div>
          <div class="col-9-md flex-container">
          <!-- ko foreach: ComparableBoats  -->
          <div class="compare-value col-3-md">
            <ul data-bind="foreach: PropulsionNames">
              <li data-bind="text: $data"></li>
            </ul>
          </div>
          <!-- /ko -->
          <!-- ko if: emptyComparisonSlot -->
          <div class="compare-header-empty-holder">
