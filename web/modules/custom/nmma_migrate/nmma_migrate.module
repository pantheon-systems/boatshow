<?php

/**
 * @file
 * Contains nmma_migrate.module.
 */

/**
 * Used as a callback process plugin value to create a link field.
 *
 * @param string $url
 *   The URL for the link.
 *
 * @return array
 *   The values needed for a link field.
 */
function nmma_migrate_link_create($url) {
  return ['url' => $url, 'title' => $url];
}
